* Problem 4
** 
   #+BEGIN_SRC asm
   la    $s0, pow
   lw    $s0, 0($s0)

   addi  $t0, $zero, 1
   addi  $t1, $zero, 0
LOOP:
   slt   $t2, $s0, $t1
   bne   $t2, $zero, LOOP_END
   sll   $t0, $t0, 1
   addi  $t1, $t1, 1
   j     LOOP
LOOP_END:
   #+END_SRC 
