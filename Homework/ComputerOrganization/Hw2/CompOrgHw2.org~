   #+BEGIN_SRC asm
   la     $t3, cbq
   lw     $t3, 0($t3)
   sub    $t1, $s1, $s0
   la     $s5, bnsf
   lw     $s5, 0($s5)
   sub    $t2, $s5, $s2
   slt    $t0, $t2, $t1
   bne    $t0, $zero, ELSE
   sw     $t3, 0($t2)
   ELSE:
   #+END_SRC
** 
   #+BEGIN_SRC asm
   la    $t0, erie
   sw    $t0, 0($t0)
   la    $t1, epsw
   sw    $t1, 0($t1)

   slt   $t7, $s0, $t0
   bne   $t7, $zero, ELSE
   slt   $t7, $t0, $t1
   bne   $t7, $zero, ELSE
   sw    $s0, 0($t1)
   ELSE:
   #+END_SRC 
